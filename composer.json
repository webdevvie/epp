{
    "name": "webdevvie\/epp",
    "description": "An extendable PHP epp client",
    "version": "0.3.0",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "EPP",
        "language",
        "serialization"
    ],
    "authors": [
        {
            "name": "John Bakker",
            "email": "me@johnbakker.name"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "doctrine/annotations": "^1.14|^2.0",
        "jms/serializer": "^3.3",
        "webdevvie/nestis": "^1.0"
    },
    "require-dev": {
        "mockery\/mockery": "^1.3",
        "phpmd\/phpmd": "^2.8.2",
        "phpstan\/phpstan": "^1.10",
        "phpunit\/phpunit": "^9.6",
        "squizlabs\/php_codesniffer": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Webdevvie\\Epp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webdevvie\\Epp\\Tests\\": "Tests\/"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
